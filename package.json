{
    "name": "html-spec-tags",
    "version": "0.0.1",
    "description": "All HTML tags supported by the current spec. With types!",
    "keywords": [
        "html",
        "spec",
        "tag",
        "element"
    ],
    "homepage": "https://github.com/electrovir/html-spec-tags",
    "bugs": {
        "url": "https://github.com/electrovir/html-spec-tags/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/electrovir/html-spec-tags"
    },
    "license": "(MIT or CC0 1.0)",
    "author": {
        "name": "electrovir",
        "url": "https://github.com/electrovir"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "compile": "virmator compile",
        "docs": "virmator docs",
        "format": "virmator format",
        "publish": "virmator publish \"npm run compile && npm run test:all\"",
        "test": "virmator test-web",
        "test:all": "concurrently --colors --kill-others-on-fail -c auto --names types,tests,spelling,format,docs \"npm run test:types\" \"npm run test:coverage\" \"npm run test:spelling\" \"npm run test:format\" \"npm run test:docs\"",
        "test:coverage": "npm run test coverage",
        "test:docs": "virmator docs check",
        "test:format": "virmator format check",
        "test:spelling": "virmator spellcheck",
        "test:types": "tsc --noEmit"
    },
    "devDependencies": {
        "@augment-vir/browser-testing": "^19.0.2",
        "@augment-vir/common": "^19.0.2",
        "@open-wc/testing": "^3.2.0",
        "@types/mocha": "^10.0.2",
        "@web/dev-server-esbuild": "^0.4.1",
        "@web/test-runner": "^0.17.1",
        "@web/test-runner-commands": "^0.8.0",
        "@web/test-runner-playwright": "^0.10.1",
        "@web/test-runner-visual-regression": "^0.8.2",
        "cspell": "^7.3.6",
        "istanbul-smart-text-reporter": "^1.1.2",
        "markdown-code-example-inserter": "^0.3.2",
        "prettier": "^2.8.8",
        "prettier-plugin-interpolated-html-tags": "^0.0.4",
        "prettier-plugin-jsdoc": "^0.4.2",
        "prettier-plugin-multiline-arrays": "^2.0.0",
        "prettier-plugin-organize-imports": "^3.2.3",
        "prettier-plugin-packagejson": "^2.4.5",
        "prettier-plugin-sort-json": "^2.0.0",
        "prettier-plugin-toml": "^0.3.5",
        "type-fest": "^4.3.2",
        "typedoc": "^0.25.1",
        "typescript": "^5.2.2",
        "virmator": "^8.0.1"
    }
}
